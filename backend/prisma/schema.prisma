generator client {
  provider = "prisma-client-js"
	binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  role     String
  createdAt DateTime @default(now())
}
